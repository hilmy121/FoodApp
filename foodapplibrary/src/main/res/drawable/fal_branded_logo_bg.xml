<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/falColorSecondaryVariant" />
        </shape>
    </item>
    <item>
        <bitmap
                android:src="@drawable/fal_icons_background_pattern"
                android:tileMode="repeat"
                android:tint="@color/falColorOnBackground"
        />
    </item>
    <item>
        <bitmap
        android:src="@drawable/fal_branded_logo"
        android:tileMode="disabled"
        android:gravity="center"
            />
    </item>
</layer-list>